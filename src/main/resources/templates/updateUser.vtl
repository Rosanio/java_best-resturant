<h2>Update $useer.getUsername()'s Permission'</h2>

<form class="form-group" action="/$user.getId()/users/$useer.getId()/updated" method="post">
  <select name="permission">
    <option value="admin">Admin</option>
    <option value="user">User</option>
  </select>
  <button type="submit" class="btn btn-default">Update</button>
</form>


<a href="/$user.getId()/users">Go Back</a>
